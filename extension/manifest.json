{
  "manifest_version": 2,

  "name": "Intrusion prevention system",
  "description": "Helps the security of your browser.",
  "version": "0.1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "options_page": "options.html",

  "permissions": [
    "tabs",
    "storage",
    "experimental",
    "https://*/*", 
    "http://*/*"
  ],
  "page_action": {
    "default_icon": "icon-19.png",
    "default_popup": "popup.html"
  }
}