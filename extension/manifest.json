{
  "manifest_version": 2,

  "name": "Intrusion prevention system",
  "description": "Helps the security of your browser.",
  "version": "0.0.1",

  "background": {
    "scripts": ["background.js"]
  },

  "options_page": "options.html",

  "permissions": [
    "tabs",
    "storage",
    "experimental",
    "https://*/*", 
    "http://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}